{
  "name": "waka-shortcut-time-stats",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-visualizer --open --",
    "debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "ui": "vitest --ui",
    "test:unit": "vitest",
    "test:ui": "playwright test --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "webhint": "hint",
    "sb:start": "supabase start && pnpm sb:dump && pnpm sb:reset && pnpm sb:seed && pnpm sb:types",
    "sb:dump": "supabase db dump --data-only -f supabase/seed.sql",
    "sb:types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "sb:reset": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/reset.sql",
    "sb:seed": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/seed.sql",
    "codegen": "rm -rf src/lib/generated/openapi/shortcut; openapi-generator-cli generate -i https://developer.shortcut.com/api/rest/v3/shortcut.swagger.json -o src/lib/generated/openapi/shortcut -g typescript-fetch --additional-properties=supportsES6=true,npmVersion=6.9.0,typescriptThreePlus=true"
  },
  "devDependencies": {
    "@edge-runtime/vm": "5.0.0",
    "@playwright/test": "1.57.0",
    "@supabase/supabase-js": "2.89.0",
    "@sveltejs/adapter-vercel": "6.2.0",
    "@sveltejs/kit": "2.49.2",
    "@tailwindcss/forms": "0.5.11",
    "@tailwindcss/typography": "0.5.19",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/svelte": "github:yanick/svelte-testing-library#without-babel",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.21",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "@vercel/analytics": "1.6.1",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/coverage-istanbul": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "autoprefixer": "10.4.23",
    "axios": "1.13.2",
    "daisyui": "4.12.24",
    "dayjs": "1.11.19",
    "echarts": "6.0.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-svelte3": "4.0.0",
    "hint": "7.1.13",
    "iconify-icon": "3.0.2",
    "jsdom": "27.4.0",
    "lodash": "4.17.21",
    "msw": "2.12.7",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-svelte": "3.4.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "sharp": "0.34.5",
    "svelte": "5.46.1",
    "svelte-check": "4.3.5",
    "svelte-fragment-component": "1.2.0",
    "svelte-htm": "1.2.0",
    "svelte-media": "0.1.5",
    "tailwindcss": "3",
    "terser": "5.44.1",
    "tippy.js": "6.3.7",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-inspect": "11.3.3",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  },
  "engines": {
    "node": "^24.12.0",
    "npm": "^11.6.2"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "10.32.1",
    "@supabase/ssr": "0.8.0",
    "@sveltejs/vite-plugin-svelte": "6.2.1"
  }
}
