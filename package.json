{
  "name": "waka-shortcut-time-stats",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-visualizer --open --",
    "debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "ui": "vitest --ui",
    "test:unit": "vitest",
    "test:ui": "playwright test --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "webhint": "hint",
    "sb:start": "supabase start && pnpm sb:dump && pnpm sb:reset && pnpm sb:seed && pnpm sb:types",
    "sb:dump": "supabase db dump --data-only -f supabase/seed.sql",
    "sb:types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "sb:reset": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/reset.sql",
    "sb:seed": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/seed.sql",
    "codegen": "rm -rf src/lib/generated/openapi/shortcut; openapi-generator-cli generate -i https://developer.shortcut.com/api/rest/v3/shortcut.swagger.json -o src/lib/generated/openapi/shortcut -g typescript-fetch --additional-properties=supportsES6=true,npmVersion=6.9.0,typescriptThreePlus=true"
  },
  "devDependencies": {
    "@edge-runtime/vm": "3.2.0",
    "@playwright/test": "1.51.1",
    "@supabase/auth-helpers-sveltekit": "0.13.0",
    "@supabase/supabase-js": "2.49.4",
    "@sveltejs/adapter-vercel": "3.1.0",
    "@sveltejs/kit": "1.30.4",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/svelte": "github:yanick/svelte-testing-library#without-babel",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vercel/analytics": "1.5.0",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/coverage-istanbul": "0.34.6",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.7",
    "autoprefixer": "10.4.21",
    "axios": "1.8.4",
    "daisyui": "3.9.4",
    "dayjs": "1.11.13",
    "echarts": "5.6.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-svelte3": "4.0.0",
    "hint": "7.1.13",
    "iconify-icon": "1.0.8",
    "jsdom": "22.1.0",
    "lodash": "4.17.21",
    "msw": "1.3.5",
    "postcss": "8.5.3",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "sharp": "0.34.0",
    "svelte": "4.2.19",
    "svelte-check": "3.8.6",
    "svelte-fragment-component": "1.2.0",
    "svelte-htm": "1.2.0",
    "svelte-media": "0.1.5",
    "tailwindcss": "3.4.17",
    "terser": "5.39.0",
    "tippy.js": "6.3.7",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "vite": "4.5.12",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-inspect": "0.10.6",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": "^18.12.1",
    "npm": "^8.19.2"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "7.120.3",
    "@sveltejs/vite-plugin-svelte": "3.1.2"
  }
}
