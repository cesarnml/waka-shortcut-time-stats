{
  "name": "waka-shortcut-time-stats",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-visualizer --open --",
    "debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "ui": "vitest --ui",
    "test:unit": "vitest",
    "test:ui": "playwright test --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "webhint": "hint",
    "sb:start": "supabase start && pnpm sb:dump && pnpm sb:reset && pnpm sb:seed && pnpm sb:types",
    "sb:dump": "supabase db dump --data-only -f supabase/seed.sql",
    "sb:types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "sb:reset": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/reset.sql",
    "sb:seed": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/seed.sql",
    "codegen": "rm -rf src/lib/generated/openapi/shortcut; openapi-generator-cli generate -i https://developer.shortcut.com/api/rest/v3/shortcut.swagger.json -o src/lib/generated/openapi/shortcut -g typescript-fetch --additional-properties=supportsES6=true,npmVersion=6.9.0,typescriptThreePlus=true"
  },
  "devDependencies": {
    "@edge-runtime/vm": "3.2.0",
    "@playwright/test": "1.42.1",
    "@supabase/auth-helpers-sveltekit": "0.12.0",
    "@supabase/supabase-js": "2.39.7",
    "@sveltejs/adapter-vercel": "5.1.0",
    "@sveltejs/kit": "2.5.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/svelte": "github:yanick/svelte-testing-library#without-babel",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.14.202",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vercel/analytics": "1.2.2",
    "@vitest/coverage-c8": "0.31.4",
    "@vitest/coverage-istanbul": "1.3.1",
    "@vitest/coverage-v8": "1.3.1",
    "@vitest/ui": "1.3.1",
    "autoprefixer": "10.4.18",
    "axios": "1.6.7",
    "daisyui": "4.7.2",
    "dayjs": "1.11.10",
    "echarts": "5.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte3": "4.0.0",
    "hint": "7.1.10",
    "iconify-icon": "2.0.0",
    "jsdom": "24.0.0",
    "lodash": "4.17.21",
    "msw": "2.2.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.2",
    "prettier-plugin-tailwindcss": "0.5.12",
    "sharp": "0.33.2",
    "svelte": "4.2.12",
    "svelte-check": "3.6.6",
    "svelte-fragment-component": "1.2.0",
    "svelte-htm": "1.2.0",
    "svelte-media": "0.1.5",
    "tailwindcss": "3.4.1",
    "terser": "5.29.1",
    "tippy.js": "6.3.7",
    "tslib": "2.6.2",
    "typescript": "5.4.2",
    "vite": "5.1.5",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-inspect": "0.8.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  },
  "engines": {
    "node": "^18.12.1",
    "npm": "^8.19.2"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "7.105.0",
    "@sveltejs/vite-plugin-svelte": "3.0.2"
  }
}
