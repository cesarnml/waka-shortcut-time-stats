{
  "name": "waka-shortcut-time-stats",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-visualizer --open --",
    "debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "ui": "vitest --ui",
    "test:unit": "vitest",
    "test:ui": "playwright test --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "webhint": "hint",
    "sb:start": "supabase start && pnpm sb:dump && pnpm sb:reset && pnpm sb:seed && pnpm sb:types",
    "sb:dump": "supabase db dump --data-only -f supabase/seed.sql",
    "sb:types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "sb:reset": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/reset.sql",
    "sb:seed": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/seed.sql",
    "codegen": "rm -rf src/lib/generated/openapi/shortcut; openapi-generator-cli generate -i https://developer.shortcut.com/api/rest/v3/shortcut.swagger.json -o src/lib/generated/openapi/shortcut -g typescript-fetch --additional-properties=supportsES6=true,npmVersion=6.9.0,typescriptThreePlus=true"
  },
  "devDependencies": {
    "@edge-runtime/vm": "3.0.0",
    "@playwright/test": "1.34.3",
    "@sentry/node": "7.53.1",
    "@sentry/svelte": "7.53.1",
    "@sentry/vite-plugin": "2.2.0",
    "@supabase/auth-helpers-sveltekit": "0.10.0",
    "@supabase/supabase-js": "2.23.0",
    "@sveltejs/adapter-vercel": "3.0.0",
    "@sveltejs/kit": "1.20.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@testing-library/dom": "9.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/svelte": "3.2.2",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.1",
    "@types/lodash": "4.14.195",
    "@types/testing-library__jest-dom": "5.14.6",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@vercel/analytics": "1.0.1",
    "@vitest/coverage-c8": "0.31.1",
    "@vitest/ui": "0.31.1",
    "autoprefixer": "10.4.14",
    "axios": "1.4.0",
    "daisyui": "2.52.0",
    "dayjs": "1.11.7",
    "echarts": "5.4.2",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "hint": "7.1.8",
    "iconify-icon": "1.0.7",
    "jsdom": "22.1.0",
    "lodash": "4.17.21",
    "msw": "1.2.1",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "prettier-plugin-tailwindcss": "0.3.0",
    "sharp": "0.28.0",
    "svelte": "3.59.1",
    "svelte-check": "3.4.3",
    "svelte-fragment-component": "1.2.0",
    "svelte-htm": "1.2.0",
    "svelte-media": "0.1.5",
    "tailwindcss": "3.3.2",
    "terser": "5.17.6",
    "tippy.js": "6.3.7",
    "tslib": "2.5.2",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-inspect": "0.7.28",
    "vitest": "0.31.1"
  },
  "engines": {
    "node": "^18.12.1",
    "npm": "^8.19.2"
  },
  "type": "module"
}
